<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
    "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
    
<urlrewrite>
	<rule>
		<from>^/404\.jsp(.*)</from>
		<to>/view/public/404.jsp</to>
	</rule>
	<rule>
		<from>^/500\.jsp(.*)</from>
		<to>/view/public/500.jsp$1</to>
	</rule>
	<!--<rule>-->
		<!--<from>^/example\.jsp(.*)</from>-->
		<!--<to>/view/example/dashboard/dashboard.jsp$1</to>-->
	<!--</rule>-->
</urlrewrite> 